{
  "name": "save-hooks",
  "main": "./lib/index.js",
  "version": "0.1.1",
  "author": "Mathias Beugnon <mathias@beugnon.fr>",
  "description": "Atom save hooks made easy",
  "repository": "https://github.com/oomathias/atom-save-hooks",
  "license": "MIT",
  "private": true,
  "keywords": [
    "save",
    "command",
    "commands",
    "task",
    "tasks",
    "trigger",
    "watch",
    "watcher",
    "run",
    "hook",
    "execute",
    "autorun",
    "sync",
    "node",
    "npm",
    "yarn",
    "bin"
  ],
  "package-deps": [
    "busy-signal"
  ],
  "consumedServices": {
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    }
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "printWidth": 100,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "renke"
    }
  },
  "on-save": {
    "*.js": [
      "import-sort --write"
    ],
    "*.(md|js|json)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "atom-package-deps": "^5.0.0",
    "cosmiconfig": "^5.2.0",
    "debug": "^4.1.1",
    "execa": "^1.0.0",
    "lodash.debounce": "^4.0.8",
    "micromatch": "^4.0.1",
    "string-argv": "^0.1.2"
  },
  "devDependencies": {
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-style-renke": "^6.0.0",
    "prettier": "^1.16.4"
  }
}
